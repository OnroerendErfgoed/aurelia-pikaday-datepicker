{
  "name": "aurelia-pikaday-datepicker",
  "version": "0.1.0",
  "description": "An Aurelia datepicker plugin written in Typescript that is a wrapper around Pikaday.",
  "keywords": [
    "aurelia",
    "datepicker",
    "plugin",
    "typescript",
    "Pikaday"
  ],
  "homepage": "https://github.com/OnroerendErfgoed/aurelia-pikaday-datepicker",
  "bugs": {
    "url": "https://github.com/OnroerendErfgoed/aurelia-pikaday-datepicker/issues"
  },
  "license": "MIT",
  "author": "Onroerend Erfgoed <ict@onroerenderfgoed.be> (https://www.onroerenderfgoed.be/)",
  "main": "dist/commonjs/aurelia-skeleton-plugin-ts-webpack.js",
  "typings": "dist/commonjs/aurelia-skeleton-plugin-ts-webpack.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/OnroerendErfgoed/aurelia-pikaday-datepicker"
  },
  "scripts": {
    "lint": "cross-env ./node_modules/.bin/nps lint",
    "test": "cross-env ./node_modules/.bin/nps test",
    "test-watch": "cross-env ./node_modules/.bin/nps test.watch",
    "test-debug": "cross-env ./node_modules/.bin/nps test.watch.debug",
    "prebuild": "gulp",
    "build": "cross-env ./node_modules/.bin/nps build.dist",
    "postbuild": "concurrently \"npm run copy:amd\" \"npm run copy:commonjs\" \"npm run copy:es2015\" \"npm run copy:es2017\" \"npm run copy:native-modules\" \"npm run copy:system\"",
    "copy:amd": "cross-env copyfiles --up 1 src/**/*.html src/**/*.css src/**/*.scss dist/amd",
    "copy:commonjs": "cross-env copyfiles --up 1 src/**/*.html src/**/*.css src/**/*.scss dist/commonjs",
    "copy:es2015": "cross-env copyfiles --up 1 src/**/*.html src/**/*.css src/**/*.scss dist/es2015",
    "copy:es2017": "cross-env copyfiles --up 1 src/**/*.html src/**/*.css src/**/*.scss dist/es2017",
    "copy:native-modules": "cross-env copyfiles --up 1 src/**/*.html src/**/*.css src/**/*.scss dist/native-modules",
    "copy:system": "cross-env copyfiles --up 1 src/**/*.html src/**/*.css src/**/*.scss dist/system",
    "demo-dev": "cross-env ./node_modules/.bin/nps build.demo",
    "demo-prod": "cross-env ./node_modules/.bin/nps build.demo.production",
    "release-patch": "cross-env ./node_modules/.bin/nps release.patch",
    "release-minor": "cross-env ./node_modules/.bin/nps release.minor",
    "release-major": "cross-env ./node_modules/.bin/nps release.major",
    "release-patch-dry": "cross-env ./node_modules/.bin/nps release.patch.dry",
    "release-minor-dry": "cross-env ./node_modules/.bin/nps release.minor.dry",
    "release-major-dry": "cross-env ./node_modules/.bin/nps release.major.dry",
    "ghpages": "cross-env ./node_modules/.bin/nps ghpages",
    "ghpages-setup": "cross-env ./node_modules/.bin/nps ghpages.setup"
  },
  "dependencies": {
    "@types/pikaday": "^1.6.3",
    "moment": "^2.22.0",
    "pikaday": "^1.7.0",
    "font-awesome": "^4.7.0"
  },
  "devDependencies": {
    "@types/bluebird-global": "^3.5.5",
    "@types/jasmine": "^2.8.6",
    "@types/karma": "^1.7.3",
    "@types/node": "^9.4.7",
    "@types/webpack": "^4.1.1",
    "aurelia-bootstrapper": "^2.2.0",
    "aurelia-framework": "^1.1.5",
    "aurelia-pal-browser": "^1.7.0",
    "aurelia-polyfills": "^1.3.0",
    "aurelia-testing": "^1.0.0-beta.4.0.0",
    "aurelia-webpack-plugin": "^3.0.0-rc.1",
    "bluebird": "^3.5.1",
    "copyfiles": "^2.0.0",
    "cross-env": "^5.1.4",
    "css-loader": "^0.28.10",
    "expose-loader": "^0.7.5",
    "extract-loader": "^1.0.2",
    "extract-text-webpack-plugin": "^3.0.2",
    "fork-ts-checker-webpack-plugin": "^0.4.1",
    "gulp": "^3.9.1",
    "gulp-sass": "^4.0.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.0.6",
    "istanbul-instrumenter-loader": "^3.0.0",
    "jasmine-core": "^3.1.0",
    "karma": "^2.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.1",
    "karma-coverage-istanbul-reporter": "^1.4.2",
    "karma-firefox-launcher": "^1.1.0",
    "karma-ie-launcher": "^1.0.0",
    "karma-jasmine": "^1.1.1",
    "karma-mocha-reporter": "^2.2.5",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.13",
    "nps": "^5.8.2",
    "nps-utils": "^1.5.0",
    "standard-version": "^4.3.0",
    "style-loader": "^0.20.3",
    "ts-loader": "^4.0.1",
    "ts-node": "^5.0.1",
    "tsconfig-paths": "^3.1.1",
    "tslint": "^5.9.1",
    "tslint-microsoft-contrib": "^5.0.3",
    "typescript": "^2.7.2",
    "webpack": "^4.1.1",
    "webpack-cli": "^2.0.11",
    "webpack-dev-server": "^3.1.1"
  },
  "jspm": {
    "registry": "npm",
    "jspmPackage": true,
    "main": "index",
    "format": "amd",
    "directories": {
      "dist": "dist/amd"
    },
    "peerDependencies": {},
    "dependencies": {}
  },
  "aurelia": {
    "build": {
      "resources": [
        "aurelia-pikaday-datepicker/aurelia-pikaday-datepicker"
      ]
    },
    "import": {
      "dependencies": [
        {
          "name": "aurelia-pikaday-datepicker",
          "main": "index.js",
          "path": "../node_modules/aurelia-pikaday-datepicker/dist/amd",
          "resources": [
            "**/*.{css,html}"
          ],
          "deps": [
            "@types/pikaday",
            "pikaday",
            "moment",
            "font-awesome"
          ]
        }
      ]
    }
  }
}
